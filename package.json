{
	"name": "react-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "vite --mode development",
		"build:test": "tsc && vite build --mode test",
		"build": "tsc && vite build  --mode production",
		"preview": "vite preview",
		"lint-staged": "lint-staged",
